#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int i,j,num;
	printf("enter a number:");
	scanf("%d",&num);
	for(i=1;i<=num;i++)
{     for(j=1;j<=num;j++)
   {if ((i+j)%2==0)
	printf("\n(%d,%d)",i,j);
	}
	}	
	return 0;
}
