#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int num,i,fact=1;
	printf("enter the number to get factioral of:");
	scanf("%d",&num);
	
	for(i=num;i>=1;i--)
	{fact=fact*i;
	}
	printf("%d",fact);
	return 0;
}
