#include <stdio.h>

int main() {
    int i,num;

    printf("Enter the num for multiplication tables: ");
    scanf("%d", &num);
if(num>=1&&num<=5)
{

   
    for (i = 1; i <=10; i++) {
       {
		printf("\nTable of %d\n", i); 

        
            printf("%d x %d = %d\n", num, i,num*i );
        }
    }}
    else
    printf("\noutside the confines of the defined limit");

    return 0;
}
